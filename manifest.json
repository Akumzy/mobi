{
  "name": "Mobi",
  "version": "1.0",
  "description": "An app for viewing local web app with mobile scale",
  "manifest_version": 2,
  "permissions": ["storage", "tabs"],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Open a mobile view"
  }
}
