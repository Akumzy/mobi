{
  "name": "Mobi",
  "short_name": "Mobi",
  "version": "4.0",
  "description": "Chrome extension for loading a wep app with mobile view scale.",
  "manifest_version": 2,
  "permissions": ["storage", "tabs"],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Open a mobile view"
  },
  "icons": { "16": "icon16.png", "48": "icon48.png", "128": "icon128.png" }
}
